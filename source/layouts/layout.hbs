<!doctype html>
<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {{#if meta.title}}
    <title>{{meta.title}}</title>
    <meta property="og:title" content="{{meta.title}}" />
    <meta name="twitter:title" value="{{meta.title}}" />
    <meta name="description" itemprop="description" content="{{meta.description}}" />
    <meta property="og:description" content="{{meta.description}}" />
    <meta name="twitter:description" value="{{meta.description}}" />
    {{else}}
    {{#if page}}
    <title>{{meta.config.name}} - Page: {{page}}</title>
    <meta property="og:title" content="{{meta.config.name}} - Page: {{page}}" />
    <meta name="twitter:title" value="{{meta.config.name}} - Page: {{page}}" />
    <meta name="description" itemprop="description" content="{{meta.config.name}} - Page: {{page}}" />
    <meta property="og:description" content="{{meta.config.name}} - Page: {{page}}" />
    <meta name="twitter:description" value="{{meta.config.name}} - Page: {{page}}" />
    {{/if}}
    {{#if tag}}
    <title>{{meta.config.name}} - Tag: {{tag}}</title>
    <meta property="og:title" content="{{meta.config.name}} - Tag: {{tag}}" />
    <meta name="twitter:title" value="{{meta.config.name}} - Tag: {{tag}}" />
    <meta name="description" itemprop="description" content="{{meta.config.name}} - Posts containing tag: {{tag}}" />
    <meta property="og:description" content="{{meta.config.name}} - Posts containing tag: {{tag}}" />
    <meta name="twitter:description" value="{{meta.config.name}} - Posts containing tag: {{tag}}" />
    {{/if}}
    {{/if}}
    <link rel="canonical" href="{{meta.url}}" />
    <link rel="icon" type="image/png" href="/content/images/favicon.png" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="{{meta.url}}" />
    <meta property="og:site_name" content="{{meta.config.name}}" />
    <meta name="twitter:card" value="summary" />
    <meta name="twitter:url" value="{{meta.url}}" />
    {{#if meta.image}}
    <meta name="twitter:image" value="<{{meta.image}}" />
    {{/if}}
    {{#if meta.config.twitterHandle}}
    <meta name="twitter:site" value="{{meta.config.twitterHandle}}" />
    {{/if}}
    <link rel="alternate" type="application/rss+xml" href="{{meta.config.baseUrl}}/rss" />
    <link rel="alternate" type="application/atom+xml" href="{{meta.config.baseUrl}}/atom" />
    <link rel="alternate" type="application/json" href="{{meta.config.baseUrl}}/json" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.0.0-beta2/css/bootstrap.min.css" integrity="sha512-aqT9YD5gLuLBr6ipQAS+72o5yHKzgZbGxEh6iY8lW/r6gG14e2kBTAJb8XrxzZrMOgSmDqtLaF76T0Z6YY2IHg==" crossorigin="anonymous" />
    <link rel="stylesheet" href="/content/stylesheets/style{{meta.config.fileEnv}}.css">
    <link rel="stylesheet" href="/content/stylesheets/syntax{{meta.config.fileEnv}}.css">
    <script src="/content/javascripts/site{{meta.config.fileEnv}}.js"></script>
  </head>
  <body>
    <header class="d-flex flex-wrap justify-content-center py-3 border-bottom">
      <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-dark text-decoration-none">
        <img src="/content/images/squido.svg" alt="" width="60" height="60" class="d-inline-block align-text-top">
        <span class="fs-4 brand">squido docs</span>
      </a>
      <ul class="nav nav-pills p-1">
        <li class="nav-item p-2"><a href="/" class="nav-link btn-outline-secondary">Home</a></li>
        <li class="nav-item p-2"><a target="_blank" href="https://github.com/mrvautin/squido" class="nav-link btn-outline-secondary">Github</a></li>
      </ul>
    </header>
    <main class="container-fluid">
      {{{body}}}
    </main>
  </body>
</html>